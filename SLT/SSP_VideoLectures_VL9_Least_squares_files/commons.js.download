table-setting textarea.value-edit.prompt:focus {\n  padding: 5px;\n}\n.auto-play-detector {\n  position: absolute;\n  top: 0;\n  left: 0;\n  width: 1px;\n  height: 1px;\n  z-index: 10000000;\n}\n.screen-reader-only {\n  position: absolute;\n  padding: 0;\n  height: 1px;\n  width: 1px;\n  margin: -1px;\n  overflow: hidden;\n  clip: rect(1px, 1px, 1px, 1px);\n  clip-path: inset(50%);\n}\n.break {\n  display: block;\n  height: 15px;\n}\n.clear {\n  clear: both;\n  height: 0px;\n}\n.error {\n  color: #990000 !important;\n}\n.ellipsis {\n  white-space: nowrap;\n  overflow: hidden;\n  text-overflow: ellipsis;\n}\n.attention {\n  font-weight: bold;\n}\n.hidden-scrollbar {\n  /* Chrome/Safari/Webkit */\n  scrollbar-width: none;\n  /* Firefox */\n}\n.hidden-scrollbar::-webkit-scrollbar {\n  display: none;\n}\n.property-page-group {\n  display: flex;\n}\n.property-page-group .property-page-tabs {\n  width: 125px;\n  padding-top: 22px;\n  padding-left: 0px;\n  font-size: 0.9em;\n}\n.property-page-group .property-page-tabs li {\n  text-align: left;\n  padding: 3px 4px 4px 20px;\n  list-style: none;\n  color: #777777;\n  cursor: pointer;\n}\n.property-page-group .property-page-tabs li:hover,\n.property-page-group .property-page-tabs li.focus {\n  background-color: 20px;\n}\n.property-page-group .property-page-tabs li.selected {\n  border-left: 5px solid;\n  padding-left: 15px;\n  color: white;\n  font-weight: bold;\n}\n.property-page-group .property-page-tabs li.disabled {\n  opacity: 0.3;\n  cursor: default;\n}\n","",{version:3,sources:["webpack://./React/Shared/Components/PropertyPages/PropertyPageGroup.less","webpack://./../../WebCore/css/Assets/Assets.less","webpack://./../../WebCore/css/Assets/Components.less","webpack://./../../WebCore/css/Assets/Mixins.less"],names:[],mappings:"AAAA,+CAA+C;AAC/C,UAAU;AACV,UAAU;AACV,kBAAkB;AAClB,6EAA6E;AAC7E,gBAAgB;AAChB,gCAAgC;AAChC,wBAAwB;AACxB,WAAW;AACX,yBAAyB;AACzB,oBAAoB;AACpB,mCAAmC;AACnC,cAAc;AACd,YAAY;AACZ,wBAAwB;AACxB,WAAW;AACX,gB